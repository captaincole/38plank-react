{
  "name": "plank38",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.15",
    "@types/react-test-renderer": "^16.0.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "jest": "^23.0.0",
    "react-native-typescript-transformer": "^1.2.5",
    "react-test-renderer": "16.3.1",
    "ts-jest": "^22.4.6",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "dependencies": {
    "@types/react-native-navigation": "^1.1.11",
    "@types/react-navigation": "^1.5.11",
    "@types/react-router-native": "^4.2.2",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-native-navigation": "^1.1.463",
    "react-native-snap-carousel": "^3.7.0"
  }
}
